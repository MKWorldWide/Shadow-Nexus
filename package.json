{
  "name": "shadow-nexus",
  "version": "1.0.0",
  "description": "A Project Blessed by Solar Khan & Lilith.Aethra - Multi-Discord Webhook Command Center",
  "main": "src/bot/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "node src/bot/index.js",
    "dev": "NODE_ENV=development nodemon src/bot/index.js",
    "migrate": "node migrations/run-migrations.js",
    "test": "NODE_ENV=test jest --coverage --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:ci": "npm test -- --ci --runInBand --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepare": "husky install",
    "security": "npm audit --production",
    "validate": "npm run lint && npm run format:check && npm run test:ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MKWorldWide/Shadow-Nexus.git"
  },
  "keywords": [
    "discord",
    "webhook",
    "automation",
    "orchestration",
    "shadow-nexus"
  ],
  "author": "MKWorldWide",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MKWorldWide/Shadow-Nexus/issues"
  },
  "homepage": "https://github.com/MKWorldWide/Shadow-Nexus#readme",
  "dependencies": {
    "@discordjs/rest": "^2.5.1",
    "discord-api-types": "^0.37.39",
    "discord.js": "^14.14.1",
    "dotenv": "^17.2.1",
    "sqlite3": "^5.1.6",
    "sequelize": "^6.35.1",
    "winston": "^3.11.0",
    "axios": "^1.6.0",
    "node-cron": "^4.2.1",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^30.0.5",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
